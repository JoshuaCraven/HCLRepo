package com.hcl.assignment2;

public class PrintPrime {
	public static void main(String[] args) {
		int x = 11;
		boolean z = true;
		int temp=x-5;
		
		
		for(int i=2; i<=x/2;++i) {
			if(x%i==0) {
				z=false;
				break;
			}
		}
		if (z==true) {
			System.out.println(x+ " is a prime number.");
		}
		else {
			System.out.println(x+ " is not a prime number.");
		}
	}

}
