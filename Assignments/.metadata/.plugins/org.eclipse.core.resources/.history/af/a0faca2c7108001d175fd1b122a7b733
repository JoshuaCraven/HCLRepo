package com.hcl.assignment2;
import java.util.Scanner;

public class SumOfTwo {
	
	public static boolean main(String[] args) {

		Scanner scc =new Scanner(System.in);
		int x= scc.nextInt();
		int y= scc.nextInt();
		int z= scc.nextInt();
		
		if (x+y==z) {
			return true;
		}
		else {return false;}
}
	
}
